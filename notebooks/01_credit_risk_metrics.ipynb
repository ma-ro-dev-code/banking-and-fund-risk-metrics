   "source": [
    "# 2. Portfolio-level Expected Loss (absolute and as % of EAD (exposure at default))\n",
    "\n",
    "total_ead = df_el[\"ead\"].sum()\n",
    "total_el = df_el[\"expected_loss\"].sum()\n",
    "\n",
    "el_pct_of_ead = total_el / total_ead * 100\n",
    "\n",
    "print(\"=== Portfolio Expected Loss ===\")\n",
    "print(f\"Total EAD:           {total_ead:,.2f}\")\n",
    "print(f\"Total Expected Loss: {total_el:,.2f}\")\n",
    "print(f\"EL as % of EAD:      {el_pct_of_ead:.2f}%\")\n",
    "\n",
    "# 3. Segment-level Expected Loss (Retail / SME / Corporate)\n",
    "\n",
    "segment_agg = (\n",
    "    df_el\n",
    "    .groupby(\"segment\")[[\"ead\", \"expected_loss\"]]\n",
    "    .sum()\n",
    "    .reset_index()\n",
    ")\n",
    "\n",
    "segment_agg[\"el_pct_of_ead\"] = segment_agg[\"expected_loss\"] / segment_agg[\"ead\"] * 100\n",
    "\n",
    "segment_agg"
   ]
