    "import numpy as np\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# ===== 0. Work on a copy to keep original df clean =====\n",
    "df_cov = df.copy()\n",
    "\n",
    "# ===== 1. Expected Loss and Coverage Ratio per loan =====\n",
    "# If expected_loss already exists, this will just overwrite with the same formula\n",
    "df_cov[\"expected_loss\"] = df_cov[\"pd_1y\"] * df_cov[\"lgd\"] * df_cov[\"ead\"]\n",
    "\n",
    "# Avoid division by zero: where expected_loss == 0, set coverage_ratio = NaN\n",
    "df_cov[\"coverage_ratio\"] = np.where(\n",
    "    df_cov[\"expected_loss\"] > 0,\n",
    "    df_cov[\"provision_amount\"] / df_cov[\"expected_loss\"],\n",
    "    np.nan,\n",
    ")\n",
    "\n",
    "print(\"=== Sample of per-loan coverage ratio ===\")\n",
    "print(\n",
    "    df_cov[\n",
    "        [\n",
    "            \"loan_id\",\n",
    "            \"segment\",\n",
    "            \"pd_1y\",\n",
    "            \"lgd\",\n",
    "            \"ead\",\n",
    "            \"expected_loss\",\n",
    "            \"provision_amount\",\n",
    "            \"coverage_ratio\",\n",
    "        ]\n",
    "    ]\n",
    "    .head(5)\n",
    "    .to_string(index=False)\n",
    ")\n",
    "print(\"\\n\")\n",
    "\n",
    "# ===== 2. Portfolio-level and segment-level coverage (base scenario) =====\n",
    "portfolio_ead = df_cov[\"ead\"].sum()\n",
    "portfolio_el = df_cov[\"expected_loss\"].sum()\n",
    "portfolio_prov = df_cov[\"provision_amount\"].sum()\n",
    "portfolio_cov = portfolio_prov / portfolio_el if portfolio_el > 0 else np.nan\n",
    "\n",
    "print(\"=== Portfolio-level coverage (base scenario) ===\")\n",
    "print(f\"Total EAD:            {portfolio_ead:,.2f}\")\n",
    "print(f\"Total Expected Loss:  {portfolio_el:,.2f}\")\n",
    "print(f\"Total Provisions:     {portfolio_prov:,.2f}\")\n",
    "print(f\"Coverage Ratio (Prov / EL): {portfolio_cov:,.2f}\")\n",
    "print(\"\\n\")\n",
    "\n",
    "segment_agg_base = (\n",
    "    df_cov.groupby(\"segment\")[[\"ead\", \"expected_loss\", \"provision_amount\"]]\n",
    "    .sum()\n",
    "    .rename(\n",
    "        columns={\n",
    "            \"ead\": \"total_ead\",\n",
    "            \"expected_loss\": \"total_el\",\n",
    "            \"provision_amount\": \"total_provisions\",\n",
    "        }\n",
    "    )\n",
    ")\n",
    "segment_agg_base[\"coverage_ratio\"] = (\n",
    "    segment_agg_base[\"total_provisions\"] / segment_agg_base[\"total_el\"]\n",
    ")\n",
    "\n",
    "print(\"=== Segment-level coverage (base scenario) ===\")\n",
    "print(segment_agg_base.to_string())\n",
    "print(\"\\n\")\n",
    "\n",
    "# ===== 3. Stress scenario: PD × 1.5, LGD × 1.2 (capped at 1) =====\n",
    "df_cov[\"pd_1y_stress\"] = (df_cov[\"pd_1y\"] * 1.5).clip(upper=1.0)\n",
    "df_cov[\"lgd_stress\"] = (df_cov[\"lgd\"] * 1.2).clip(upper=1.0)\n",
    "df_cov[\"expected_loss_stress\"] = (\n",
    "    df_cov[\"pd_1y_stress\"] * df_cov[\"lgd_stress\"] * df_cov[\"ead\"]\n",
    ")\n",
    "\n",
    "portfolio_el_stress = df_cov[\"expected_loss_stress\"].sum()\n",
    "portfolio_cov_stress = portfolio_prov / portfolio_el_stress if portfolio_el_stress > 0 else np.nan\n",
    "\n",
    "print(\"=== Portfolio-level coverage (stress scenario) ===\")\n",
    "print(f\"Total Expected Loss (stress):  {portfolio_el_stress:,.2f}\")\n",
    "print(f\"Total Provisions:              {portfolio_prov:,.2f}\")\n",
    "print(f\"Coverage Ratio (Prov / EL_stress): {portfolio_cov_stress:,.2f}\")\n",
    "print(\"\\n\")\n",
    "\n",
    "segment_agg_stress = (\n",
    "    df_cov.groupby(\"segment\")[[\"expected_loss_stress\", \"provision_amount\"]]\n",
    "    .sum()\n",
    "    .rename(\n",
    "        columns={\n",
    "            \"expected_loss_stress\": \"total_el_stress\",\n",
    "            \"provision_amount\": \"total_provisions\",\n",
    "        }\n",
    "    )\n",
    ")\n",
    "segment_agg_stress[\"coverage_ratio_stress\"] = (\n",
    "    segment_agg_stress[\"total_provisions\"] / segment_agg_stress[\"total_el_stress\"]\n",
    ")\n",
    "\n",
    "print(\"=== Segment-level coverage (stress scenario) ===\")\n",
    "print(segment_agg_stress[[\"total_el_stress\", \"total_provisions\", \"coverage_ratio_stress\"]].to_string())\n",
    "print(\"\\n\")\n",
    "\n",
    "# ===== 4. Combined view base vs stress (for plotting) =====\n",
    "coverage_compare = segment_agg_base[[\"coverage_ratio\"]].join(\n",
    "    segment_agg_stress[[\"coverage_ratio_stress\"]]\n",
    ")\n",
    "\n",
    "print(\"=== Segment coverage: base vs stress ===\")\n",
    "print(coverage_compare.to_string())\n",
    "print(\"\\n\")\n",
    "\n",
    "# ===== 5. Bar chart: coverage base vs stress by segment =====\n",
    "segments = coverage_compare.index.tolist()\n",
    "x = np.arange(len(segments))\n",
    "width = 0.35\n",
    "\n",
    "fig, ax = plt.subplots(figsize=(8, 4))\n",
    "ax.bar(x - width / 2, coverage_compare[\"coverage_ratio\"], width, label=\"Base\")\n",
    "ax.bar(x + width / 2, coverage_compare[\"coverage_ratio_stress\"], width, label=\"Stress\")\n",
    "\n",
    "ax.set_xticks(x)\n",
    "ax.set_xticklabels(segments)\n",
    "ax.set_ylabel(\"Coverage ratio (Provisions / Expected Loss)\")\n",
    "ax.set_title(\"Coverage ratio by segment: base vs stress\")\n",
    "ax.legend()\n",
    "ax.grid(axis=\"y\", alpha=0.3)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n"
   ]
